<section id="catalog-section" class="spaced">

    <h1 class="item">Closed Auctions</h1>

    {{#if auctions}}
    <ul class="catalog cards">

        {{#each auctions}}
        {{> closed}}
        {{/each}}

    </ul>
    {{else}}
    <main class="item pad-large align-center">
        <p>
            You haven't closed any auctions yet.
        </p>
    </main>
    {{/if}}

</section>